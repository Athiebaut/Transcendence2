{
  "name": "transcendence-back_to_back",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc -p tsconfig.json && prisma generate",
    "start": "node dist/index.js",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "web:build": "esbuild web/main.ts --bundle --outfile=web/dist/main.js --platform=browser --format=esm --target=es2020",
    "web:watch": "esbuild web/main.ts --bundle --outfile=web/dist/main.js --platform=browser --format=esm --target=es2020 --watch"
  },
  "dependencies": {
    "@fastify/formbody": "^8.0.2",
    "@fastify/cookie": "^10.0.0",
    "@fastify/cors": "^10.0.0",
    "@fastify/multipart": "^9.3.0",
    "@prisma/client": "^5.18.0",
    "argon2": "^0.41.1",
    "bcrypt": "^6.0.0",
    "fastify": "^5.2.0",
    "jose": "^5.9.3",
    "jsonwebtoken": "^9.0.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.18.12",
    "esbuild": "^0.25.9",
    "prisma": "^5.18.0",
    "tsx": "^3.12.7",
    "typescript": "^5.9.3"
  }
}